{
  "rewrites": [{ "source": "/(.*)", "destination": "/" }],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://www.videogata.com https://preview.videogata.com file://* http://localhost"
        }
      ]
    }
  ],
  "github": {
    "silent": true
  }
}
